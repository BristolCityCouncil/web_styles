  {% assign signed_in = false %}
  {% assign hide_breadcrumbs = true %}
  {% include 'header' %}

  <div class="page-content">

    <div class="layout layout-constrained">

      <div class="grid-wrap">

        <div class="grid-col desktop-grid-two-thirds">

          <div class="content content-header">

            <h1 class="content-header__title">Signed out</h1>

          </div><!--/content-->


          <div class="content content-main">

            <div class="panel panel--affirm">
              <div class="panel__copy">
                <p>You have successfully signed out.</p>
              </div>
            </div>

          </div><!--/content-->

        </div><!--/grid-col-->



      </div><!--/grid-wrap-->

    </div><!--/layout-->

  </div><!--page-content-->

  {% include 'footer' %}

{% endblock %}
